<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions name="Hello"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
   xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
   xmlns:h="http://www.example.org/schema/Hello"
   xmlns:tns="http://www.example.org/contract/Hello"
   targetNamespace="http://www.example.org/contract/Hello">
   <wsdl:types>
      <xsd:schema targetNamespace="http://www.example.org/schema/Hello">
         <xsd:element name="SayHello">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="nameToHello" type="xsd:string"/>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="SayHelloResponse">
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="personalizedHello" type="xsd:string" />
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:schema>
   </wsdl:types>
   <wsdl:message name="SayHello">
      <wsdl:part element="h:SayHello" name="parameters" />
   </wsdl:message>
   <wsdl:message name="SayHelloResponse">
      <wsdl:part element="h:SayHelloResponse" name="parameters" />
   </wsdl:message>
   <wsdl:portType name="HelloPortType">
      <wsdl:operation name="SayHello">
         <wsdl:input message="tns:SayHello" />
         <wsdl:output message="tns:SayHelloResponse" />
      </wsdl:operation>
   </wsdl:portType>
   <wsdl:binding name="HelloBinding" type="tns:HelloPortType">
      <soap:binding style="document"
         transport="http://schemas.xmlsoap.org/soap/http" />
      <wsdl:operation name="SayHello">
         <soap:operation soapAction=""/>
         <wsdl:input><soap:body use="literal"/></wsdl:input>
         <wsdl:output><soap:body use="literal"/></wsdl:output>
      </wsdl:operation>
   </wsdl:binding>
   <wsdl:service name="HelloService">
      <wsdl:port name="HelloPort" binding="tns:HelloBinding">
         <soap:address
            location="http://localhost:8080/hello/services/hello"/>
      </wsdl:port>
   </wsdl:service>
</wsdl:definitions> 